{
  "expo": {
    "name": "osc-proximity-chat-app",
    "slug": "osc-proximity-chat-app",
    "scheme": "osc-proximity-chat-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/expo/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/expo/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "34D1BF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/expo/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow OSC Proximity Chat to access your location."
        }
      ],
      "expo-font"
    ]
  }
}
